{
  "name": "file-manager-pro",
  "version": "2.2.0",
  "description": "Production-ready file upload and management system with email verification, user authentication, share links, and 100GB file support",
  "main": "server.js",
  "scripts": {
    "start": "set PORT=3001 && node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "connect-sqlite3": "^0.9.16",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "helmet": "^7.2.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "file-upload",
    "file-manager",
    "express",
    "nodejs",
    "authentication",
    "email-verification",
    "share-links",
    "large-files",
    "100gb",
    "resumable-downloads",
    "streaming",
    "gmail",
    "nodemailer"
  ],
  "author": "Bareq Maher <https://github.com/bareqmaher-arch>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bareqmaher-arch/file-manager-pro.git"
  },
  "bugs": {
    "url": "https://github.com/bareqmaher-arch/file-manager-pro/issues"
  },
  "homepage": "https://github.com/bareqmaher-arch/file-manager-pro#readme",
  "config": {
    "maxFileSize": "107374182400",
    "maxFiles": "10",
    "uploadTimeout": "7200000",
    "emailVerification": "true",
    "shareLinksExpiry": "7"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/bareqmaher-arch"
  }
}